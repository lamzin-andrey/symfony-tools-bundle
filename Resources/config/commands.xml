<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="landlib_symfony_tools.command.decorate_controller" class="Landlib\SymfonyToolsBundle\Command\DecorateControllerCommand">
            <tag name="console.command" command="landlib:decorate-controller" />
        </service>

        <service id="landlib_symfony_tools.command.service_alias" class="Landlib\SymfonyToolsBundle\Command\GetServiceAliasCommand">
            <tag name="console.command" command="landlib:service-alias" />
        </service>
    </services>

</container>
